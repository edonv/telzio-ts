{
  "compilerOptions": {
    "lib": ["ES2020"],
    "module": "Node16",
    "moduleResolution": "Node16",
    "target": "ES2020",
    "skipLibCheck": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "declaration": true
  },
  "compileOnSave": true,
  "include": [
    "src/**/*.ts",
  ],
  "exclude": [
    "swagger.json"
  ]
}
