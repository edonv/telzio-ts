  "files": [
    "/README.md",
    "/LICENSE.md",
    "/dist/"
  ],
  "dependencies": {
    "openapi-fetch": "^0.9.8"
  },
  "devDependencies": {
    "openapi-typescript": "^7.4.0",
    "typescript": "^5.5.4"
  }
