{
  "name": "telzio-ts",
  "version": "0.0.2",
  "type": "module",
  "license": "MIT",
  "description": "Node package for Telzio API v3.",
  "repository": "github:edonv/telzio-ts",
  "author": "edonv",
  "private": "false",
  "sponsor": {
    "url": "https://github.com/sponsors/edonv"
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
